{
  "id": null,
  "uid": "upload-service",
  "title": "Upload Service",
  "tags": ["upload", "service"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests / s",
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"upload-service\"}[1m])",
          "legendFormat": "{{method}} {{status}}"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Request Duration (95th percentile)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_ms_bucket{job=\"upload-service\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "logs",
      "title": "Upload Service Logs",
      "datasource": { "type": "loki", "uid": "P8E80F9AEF21F6940" },
      "targets": [
        {
          "expr": "{service=\"upload-service\"}"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 }
    }
  ]
}
